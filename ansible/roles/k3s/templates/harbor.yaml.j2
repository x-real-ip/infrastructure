  rules:
    - host: "harbor.lan.theautomation.nl"
      http:
        paths:
          - path: /api/
            pathType: Prefix
            backend:
              service:
                name: {{ harbor.name }}-core
                port:
                  number: 80
          - path: /service/
            pathType: Prefix
            backend:
              service:
                name: {{ harbor.name }}-core
                port:
                  number: 80
          - path: /v2/
            pathType: Prefix
            backend:
              service:
                name: {{ harbor.name }}-core
                port:
                  number: 80
          - path: /chartrepo/
            pathType: Prefix
            backend:
              service:
                name: {{ harbor.name }}-core
                port:
                  number: 80
          - path: /c/
            pathType: Prefix
            backend:
              service:
                name: {{ harbor.name }}-core
                port:
                  number: 80
          - path: /
            pathType: Prefix
            backend:
              service:
                name: {{ harbor.name }}-portal
                port:
                  number: 80
  tls:
    - hosts:
        - harbor.lan.theautomation.nl
      secretName: tls-wildcard-k8s-lan
