kind: Deployment
apiVersion: apps/v1
metadata:
  name: {{ harbor.name }}-core
  namespace: {{ harbor.namespace }}
  labels:
    release: {{ harbor.name }}
    app: "{{ harbor.name }}"
    component: core
spec:
  replicas: 1
  revisionHistoryLimit: 10
  selector:
    matchLabels:
      release: {{ harbor.name }}
      app: "{{ harbor.name }}"
      component: core
  template:
    metadata:
      labels:
        release: {{ harbor.name }}
        app: "{{ harbor.name }}"
        component: core
    spec:
      securityContext:
        runAsUser: 10000
        fsGroup: 10000
      automountServiceAccountToken: false
      terminationGracePeriodSeconds: 120
      priorityClassName: system-node-critical
      containers:
        - name: core
          image: goharbor/harbor-core:{{ harbor.image_tag }}
          imagePullPolicy: Always
          startupProbe:
            httpGet:
              path: /api/v2.0/ping
              scheme: HTTP
              port: 8080
            failureThreshold: 360
            initialDelaySeconds: 10
            periodSeconds: 10
          livenessProbe:
            httpGet:
              path: /api/v2.0/ping
              scheme: HTTP
              port: 8080
            failureThreshold: 2
            periodSeconds: 10
          readinessProbe:
            httpGet:
              path: /api/v2.0/ping
              scheme: HTTP
              port: 8080
            failureThreshold: 2
            periodSeconds: 10
          envFrom:
            - configMapRef:
                name: "{{ harbor.name }}-core"
            - secretRef:
                name: "{{ harbor.name }}-core"
          env:
            - name: CORE_SECRET
              valueFrom:
                secretKeyRef:
                  name: {{ harbor.name }}-core
                  key: secret
            - name: JOBSERVICE_SECRET
              valueFrom:
                secretKeyRef:
                  name: "{{ harbor.name }}-jobservice"
                  key: JOBSERVICE_SECRET
          ports:
            - containerPort: 8080
          volumeMounts:
            - name: config
              mountPath: /etc/core/app.conf
              subPath: app.conf
            - name: secret-key
              mountPath: /etc/core/key
              subPath: key
            - name: token-service-private-key
              mountPath: /etc/core/private_key.pem
              subPath: tls.key
            - name: ca-download
              mountPath: /etc/core/ca
            - name: psc
              mountPath: /etc/core/token
      volumes:
        - name: config
          configMap:
            name: {{ harbor.name }}-core
            items:
              - key: app.conf
                path: app.conf
        - name: secret-key
          secret:
            secretName: {{ harbor.name }}-core
            items:
              - key: secretKey
                path: key
        - name: token-service-private-key
          secret:
            secretName: {{ harbor.name }}-core
        - name: ca-download
          secret:
        - name: psc
          emptyDir: {}

