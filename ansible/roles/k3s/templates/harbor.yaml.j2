kind: Ingress
apiVersion: networking.k8s.io/v1
metadata:
  name: "{{ harbor.name }}-ingress"
  namespace: {{ harbor.namespace }}
  labels:
    release: {{ harbor.name }}
    app: "{{ harbor.name }}"
  annotations:
    nginx.ingress.kubernetes.io/ssl-redirect: "true"
    nginx.ingress.kubernetes.io/proxy-body-size: "0"
