  JOBSERVICE_URL: "http://{{ harbor.name }}-jobservice"
  REGISTRY_URL: "http://{{ harbor.name }}-registry:5000"
  TOKEN_SERVICE_URL: "http://{{ harbor.name }}-core:80/service/token"
  CORE_LOCAL_URL: "http://127.0.0.1:8080"
  WITH_TRIVY: "false"
  TRIVY_ADAPTER_URL: "http://{{ harbor.name }}-trivy:8080"
  REGISTRY_STORAGE_PROVIDER_NAME: "filesystem"
  LOG_LEVEL: "info"
  CONFIG_PATH: "/etc/core/app.conf"
  CHART_CACHE_DRIVER: "redis"
  _REDIS_URL_CORE: "redis://{{ harbor.name }}-redis:6379/0?idle_timeout_seconds=30"
  _REDIS_URL_REG: "redis://{{ harbor.name }}-redis:6379/2?idle_timeout_seconds=30"
  PORTAL_URL: "http://{{ harbor.name }}-portal"
  REGISTRY_CONTROLLER_URL: "http://{{ harbor.name }}-registry:8080"
  REGISTRY_CREDENTIAL_USERNAME: "theautomation"
