# NFS
storage-server-lagg.lan:/mnt/r10_8tb/media /home/coen/storage-server/media nfs rsize=32768,wsize=32768
storage-server-lagg.lan:/mnt/r10_8tb/shared /home/coen/storage-server/shared nfs rsize=32768,wsize=32768

